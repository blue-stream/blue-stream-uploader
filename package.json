{
  "name": "blue-stream-uploader",
  "version": "0.0.0",
  "description": "Blue Stream uploader for uploading video files",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/index.ts",
    "start": "node ./dist/index.js",
    "test": "mocha --recursive -r ts-node/register src/**/*.spec.ts --exit",
    "pretest": "cross-env NODE_ENV=test &&cross-env AUTHENTICATION_REQUIRED=0",
    "test:validation": "cross-env NODE_ENV=test&& mocha --recursive -r ts-node/register \"src/**/*.validator.spec.ts\" --exit",
    "test:integration": "cross-env NODE_ENV=test&& mocha --recursive -r ts-node/register \"src/**/*.router.spec.ts\" --exit",
    "test:repository": "cross-env NODE_ENV=test&& mocha --recursive -r ts-node/register \"src/**/*.repository.spec.ts\" --exit",
    "test:coverage": "cross-env NODE_ENV=test&& nyc npm test",
    "coverage-test": "cross-env NODE_ENV=test&& nyc check-coverage --lines 95 --functions 95 --branches 95",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "author": "Ron Borysovski, Almog Vagman",
  "license": "ISC",
  "devDependencies": {
    "@types/amqplib": "^0.5.8",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.38",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/multer-s3": "^2.7.6",
    "@types/node": "^10.5.3",
    "@types/passport": "^0.4.6",
    "@types/passport-jwt": "^3.0.1",
    "@types/rimraf": "^2.0.2",
    "@types/supertest": "^2.0.5",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^5.2.0",
    "node-mocks-http": "^1.7.0",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "standard-version": "^8.0.1",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "aws-sdk": "^2.249.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "elastic-apm-node": "^1.14.5",
    "express": "^4.16.3",
    "helmet": "^3.12.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "winston": "^3.2.1",
    "winston-elasticsearch": "^0.7.9"
  }
}
